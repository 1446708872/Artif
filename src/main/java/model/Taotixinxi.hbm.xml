<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Taotixinxi" table="taotixinxi" schema="exam">
        <id name="taotiId" column="taoti_id">
            <generator class="native"/>
        </id>
        <property name="taotiName" column="taoti_name"/>
        <property name="taotiLessonId" column="taoti_LessonId"/>
        <property name="taotiJoinTime" column="taoti_joinTime"/>
        <!--<property name="taotiStuId" column="taoti_stu_id"/>-->

      <many-to-one name="students" column="taoti_stu_id" class="model.Student"  not-null="true"  insert="false" update="false"/>

    </class>
</hibernate-mapping>